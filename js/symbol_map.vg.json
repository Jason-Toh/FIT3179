{
   "$schema":"https://vega.github.io/schema/vega-lite/v5.json",
   "width":800,
   "height":500,
   "title":"One dot per Robbery case in New York City",
   "projection":{
      "type":"equalEarth"
   },
   "layer":[
      {
         "data":{
            "url":"https://raw.githubusercontent.com/Jason-Toh/FIT3179/main/homework_week_9/js/newyork.topojson",
            "format":{
               "type":"topojson",
               "feature":"newyork"
            }
         },
         "mark":{
            "type":"geoshape",
            "fill":"#ffcc99",
            "stroke":"white"
         }
      },
      {
         "data":{
            "url":"https://raw.githubusercontent.com/Jason-Toh/FIT3179/main/homework_week_9/data/Robbery.csv"
         },
         "mark":{
            "type":"circle",
            "tooltip":null
         },
         "encoding":{
            "longitude":{
               "field":"longitude",
               "type":"quantitative"
            },
            "latitude":{
               "field":"latitude",
               "type":"quantitative"
            }
         }
      }
   ]
}