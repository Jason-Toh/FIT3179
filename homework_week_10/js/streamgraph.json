{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width":800,
    "height":500,
    "data":{"url":"https://raw.githubusercontent.com/Jason-Toh/FIT3179/main/homework_week_10/data/nyc_crime_filtered.csv"},
    "mark":"area",
    "encoding": {
        "x":{
            "timeUnit":"yearmonth","field":"arrest_date",
            "axis": {"domain": false, "format": "%Y", "tickSize": 0}
        },
        "y": {
            "aggregate": "count", "field": "arrest_key",
            "axis": null,
            "stack": "center"
        },
        "color": {"field":"ofns_desc", "scale":{"range": ["#003049","#d62828","#f77f00","#fcbf49","#eae2b7"]}}
    }
}